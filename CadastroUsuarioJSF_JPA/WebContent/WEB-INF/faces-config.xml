<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/loginInvalido.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>cadastroAnimal.xhtml</display-name>
		<from-view-id>/cadastroAnimal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lAnimais</from-outcome>
			<to-view-id>/listaAnimais.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>cadastroUsuario.xhtml</display-name>
		<from-view-id>/cadastroUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lUsuario</from-outcome>
			<to-view-id>/listaUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cUsuario</from-outcome>
			<to-view-id>/cadastroUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cAnimal</from-outcome>
			<to-view-id>/cadastroAnimal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lUsuario</from-outcome>
			<to-view-id>/listaUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lAnimal</from-outcome>
			<to-view-id>/listaAnimais.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
